<?xml version="1.0"?>
<GrammarLibrary>
	<GrammarObject Name="iTunes" Command="itunes,music,song" Win32="%ProgramFiles%\iTunes\iTunes.exe" Win64="%ProgramFiles(x86)%\iTunes\iTunes.exe" ></GrammarObject>
	<GrammarObject Name="Windows Explorer" Command="explorer,file" Win32="explorer.exe" Win64="explorer.exe" ></GrammarObject>
	<GrammarObject Name="Firefox" Command="firefox" Win32="%ProgramFiles%\Mozilla Firefox\firefox.exe" Win64="%ProgramFiles(x86)%\Mozilla Firefox\firefox.exe" ></GrammarObject>
	<GrammarObject Name="Google Chrome" Command="google chrome,chrome" Win32="" Win64=""></GrammarObject>
	<Argument Name="Facebook" Link="Google Chrome" Command="facebook" Win32="http://www.facebook.com" Win64="http://www.facebook.com" ></Argument>
</GrammarLibrary>